<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/btn_dialog_nor"
    android:orientation="vertical"
    android:paddingBottom="5.0dip"
    android:paddingLeft="5.0dip"
    android:paddingRight="5.0dip"
    android:paddingTop="3.0dip">

    <LinearLayout
        android:id="@+id/linear_top"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:layout_weight="1.0">

        <Button
            android:id="@+id/btnOut"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="@color/bg_menu_button"
            android:text="库存处理"
            android:textSize="14.0sp" />

        <LinearLayout
            android:id="@+id/btnSettle"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:textColor="@color/black"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="收款"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnPrint"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:textColor="@color/black"
                android:text="打印"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnDocInfo"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent">

                <TextView
                    android:layout_width="1.0dip"
                    android:layout_height="fill_parent"
                    android:background="@color/trans" />
            </LinearLayout>

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="备注"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnWritePad"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="签名"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1.0dip"
        android:background="@color/trans" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:layout_weight="1.0">

        <Button
            android:id="@+id/btnBatchDetail"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:background="@color/bg_menu_button"
            android:gravity="center"
            android:text="批次明细"
            android:textSize="14.0sp" />

        <LinearLayout
            android:id="@+id/btnSale"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="客史"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnPicture"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="照片"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnDelete"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="删除"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnLocation"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="1.0dip"
                android:layout_height="fill_parent"
                android:background="@color/trans" />

            <TextView
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@color/bg_menu_button"
                android:gravity="center"
                android:text="定位"
                android:textColor="@color/black"
                android:textSize="14.0sp" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>